foreign fn printf(cstring, int)
foreign fn fflush(&int)
foreign stdout: &int

fn main() {
   bs: [2]byte = [92, 98]
   take_byte(bs)
   xs: [2]int = [4, 11]
   take_int(xs)
  
}

fn take_int(arr: [2]int) {
   ptr: &int = &arr
   printf("%d", ^ptr)
   fflush(stdout)
}

fn take_byte(arr: [2]byte) {
   ptr: &byte = &arr
   printf("%d", ^(ptr + 1))
   fflush(stdout)
}